import 'Artist.recipes'
import '../Pipes/PipeEntity.schema'

schema PipeEntity
  Text type
  Text name

particle ArtistPipe in './source/ArtistPipe.js'
  in PipeEntity pipe
  out ArtistFind find
  consume pipes
    must provide content

recipe ArtistPipe
  use as pipe
  create as find
  create #piped #artist as artist
  create as descriptions
  ArtistPipe
    pipe = pipe
    find = find
  ArtistFinder
    find = find
    artist = artist
    descriptions = descriptions
