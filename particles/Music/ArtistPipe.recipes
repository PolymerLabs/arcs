import '../PipeApps/PipeEntity.schema'
import 'Artist.recipes'

particle ArtistPipe in './source/ArtistPipe.js'
  in PipeEntity pipe
  out ArtistFind find
  consume pipes
    provide content

recipe ArtistPipe
  use #pipe_artist as pipe
  create as find
  create #piped #artist as artist
  create as descriptions
  ArtistPipe
    pipe = pipe
    find = find
  ArtistFinder
    find = find
    artist = artist
    descriptions = descriptions
