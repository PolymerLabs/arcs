interface SlandleHostedChoiceInterface
  in ~any *
  in [~anyOther] *
  `consume Slot choice

particle SlandleChoicesMultiplexer in 'source/Multiplexer.js'
  in [~any] list
  in [~anyOther] choices
  host SlandleHostedChoiceInterface hostedParticle
  `consume [Slot] choice
