import '../Pipes/PipeEntity.schema'

schema Address
  Text address

particle RecentAddresses in './source/RecentAddresses.js'
  in [PipeEntity] recentEntities
  out Address address

recipe RecentAddresses
  map 'pipe-entities' as recentEntities
  create as address
  RecentAddresses
    recentEntities = recentEntities
    address = address
