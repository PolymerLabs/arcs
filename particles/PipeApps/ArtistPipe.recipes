import './PipeEntity.schema'
import '../Music/Artist.recipes'

particle ArtistPipe in './source/ArtistPipe.js'
  consume content
  in PipeEntity pipe
  out ArtistFind find

recipe ShowArtist
  use #artist_ as entity
  create as find
  create as descriptions
  create as artist
  ArtistPipe
    pipe = entity
    find = find
  ArtistFinder
    find = find
    artist = artist
    descriptions = descriptions
  description `show artist`
