import 'schemas/ArcMeta.schema'

particle SlandleLauncher in 'source/Launcher.js'
  inout [ArcMeta] arcs
  `consume Slot root
  description `arcs launcher`

recipe SlandleLauncher
  create 'SYSTEM_arcs' as arcs
  `slot 'rootslotid-root' as root
  SlandleLauncher
    arcs = arcs
    root consume root
