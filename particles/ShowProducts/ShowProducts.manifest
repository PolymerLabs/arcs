import '../../entities/Description.manifest'
import '../../entities/Product.manifest'

particle ShowProducts in 'ShowProducts.js'
  ShowProducts(in [Product] list, out [Description] descriptions)
  affordance dom
  affordance dom-touch
  consume root
    provide action
      view list
    provide preamble
    provide postamble
    provide set of annotation
      view list
  description `show ${list}`
