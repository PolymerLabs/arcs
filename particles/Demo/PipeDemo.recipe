import '../Pipes/PipeEntity.schema'

resource TestPipeEntityRes
  start
  //[{"type": "tv_show", "name": "star trek"}]
  [{"type": "artist", "name": "in this moment"}]

store TestPipeEntity of PipeEntity 'TestPipeEntity' @0 in TestPipeEntityRes

particle PipeEntityReceiver in '../Pipes/source/PipeEntityReceiver.js'
  in PipeEntity pipe
  consume root
    provide pipes

recipe PipeDemo
  copy 'TestPipeEntity' as pipe
  PipeEntityReceiver
  description `[demo] Pipe data test`
