import './schemas/Json.schema'
import './schemas/PipeEntity.schema'

particle CaptionThis in './source/CaptionThis.js'
  in PipeEntity query
  out Json output
  consume content

recipe CaptionThis &pipe_caption
  use #pipe_caption as query
  create as suggestion
  CaptionThis
    query = query
    output = suggestion
  description `caption this`
