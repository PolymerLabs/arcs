particle Interests in 'source/Interests.js'
  list: reads [Product {name: Text, description: Text, image: URL, category: Text, seller: Text, price: Text, shipDays: Number}]
  person: reads Person {id: Text, name: Text, occasion: Text, date: Text}
  postamble: consumes Slot
  description `find out about ${person.name}'s interests`

recipe ProductInterests
  list: use *
  person: use *
  Interests
    list: list
    person: person
