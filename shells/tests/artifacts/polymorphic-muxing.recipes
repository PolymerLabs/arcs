import './Common/List.manifest'
import './Social/Particles.manifest'
import './Social/Post.schema'

particle ShowOne in 'source/ShowPostOne.js'
  post: reads Post
  item: consumes Slot

particle ShowTwo in 'source/ShowPostTwo.js'
  post: reads Post
  item: consumes Slot

recipe
  h0: copy #recipes
  List
    items: h0
  PostMuxer
