import '../Things/Thing.schema'

schema Person extends Thing
  id: Text
  friends: Text
  foods: Text
  avatar: Text
  active: Text
  occasion: Text
  date: Text
  // TODO move back to a nested location when schemas support that.
  latitude: Number
  longitude: Number
