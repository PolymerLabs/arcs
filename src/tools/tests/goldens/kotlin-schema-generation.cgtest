  h1: reads Person Friend Parent {}
[results]
Schema(
    setOf(SchemaName("Person"), SchemaName("Friend"), SchemaName("Parent")),
    SchemaFields(
        singletons = emptyMap(),
        collections = emptyMap()
    ),
    "cd956ff7d2d4a14f434aa1427b84097d5c47037b",
    refinement = { _ -> true },
    query = null
)
[end]

