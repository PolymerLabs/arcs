{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "rootDir": "src",
    "outDir": "build",
    "types": [],
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules",
    "build",
    ".vscode-test"
  ]
}
