    "@wdio/spec-reporter": "6.4.7",
    "@wdio/sync": "6.5.0",
    "bufferutil": "4.0.1",
    "c8": "4.1.5",
    "chai": "4.2.0",
    "chromedriver": "87.0.0",
    "concurrently": "4.1.2",
    "cross-env": "5.2.1",
    "diff": "4.0.1",
    "eslint": "7.9.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-header": "3.0.0",
