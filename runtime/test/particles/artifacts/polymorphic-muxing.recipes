import '../../../../shell/artifacts/Common/List.manifest'
import '../../../../shell/artifacts/Social/Particles.manifest'
import '../../../../shell/artifacts/Social/Post.schema'

particle ShowOne in 'source/ShowPostOne.js'
  in Post post
  consume item

particle ShowTwo in 'source/ShowPostTwo.js'
  in Post post
  consume item

recipe
  map #recipes as handle
  List
    items = handle
  PostMuxer
    list = handle

resource MuxedRecipes
  start
  [
  ]

store TheRecipes of [Post] #recipes in MuxedRecipes
