test --strategy=KotlinCompile=local
test --spawn_strategy=standalone --nocache_test_results --test_output=errors --jobs=32 --ram_utilization_factor=33 
test --nostamp
test --strategy=Desugar=sandboxed --noexperimental_check_desugar_deps --nodesugar_for_android


