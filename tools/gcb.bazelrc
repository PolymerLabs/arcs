build --strategy=KotlinCompile=local
build --remote_cache=https://storage.googleapis.com/arcs-github-gcb-bazel-cache --google_default_credentials
build --verbose_failures --spawn_strategy=standalone --jobs=32 --ram_utilization_factor=33
build --nostamp
