meta
  namespace: arcs.showcase.imports.schemas

import 'common.arcs'

// Containers group `Tea` headed to the same destination
schema Container
  id: Text
  contents: [inline Tea {name, variety}]
  destination: inline Place {lat, lng}

schema Boat
  id: Text
  name: Text
  currentLocation: inline Place {lat, lng}
  nextLocations: [inline Place {lat, lng, nation}]
  containers: [&Container]
  status: Text




